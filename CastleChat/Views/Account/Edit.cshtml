
@{
    ViewData["Title"] = "Edit";
}

<h1>Edit Account</h1>
<hr>
<form method="post" enctype="multipart/form-data">
    <p>Account icon</p>
    <input name="file" type="file" style="width: 50%;" class="form-control" accept="image/*" required /><br>
    <button type="submit" class="btn btn-outline-secondary" style="width: 50%;">Update</button>
</form>
